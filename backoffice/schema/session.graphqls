type Session {

    """
    Токен авторизации, должен совпасть с тем
    что отдали клиенту при отправке формы авторизации
    """
    auth_token: String!

    """
    Идентификатор пользователя
    """
    uid:    Int!

    """
    Метод авторизации установленный пользователем
    """
    method: String!
}
